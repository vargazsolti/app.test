									"let json={};",
									"try{ json = pm.response.json(); }catch(e){}",
									"if(json && json.data && json.data.token){",
									"  pm.collectionVariables.set('token', json.data.token);",
									"  console.log('Token saved (collection variable)');",
									"} else {",
									"  console.warn('No token in response');",
									"}"
